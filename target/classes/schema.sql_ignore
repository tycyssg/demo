CREATE TABLE MODULECREATIONDB(ID INT PRIMARY KEY auto_increment, LABEL VARCHAR(50), CATNAME VARCHAR(50), CATLINK VARCHAR(255), CATDES VARCHAR(255));

CREATE TABLE PARAMSETTINGS(
  ID INT PRIMARY KEY,
  PARAMNAME VARCHAR(30) NOT NULL,
  PARAMTYPE VARCHAR(20) NOT NULL,
  FK_MODULECREATION_ID INT auto_increment,
  foreign key (FK_MODULECREATION_ID) references MODULECREATIONDB(ID)
)


